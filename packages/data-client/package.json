    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@kb-labs/devkit": "file:../../../kb-labs-devkit",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.18",
    "eslint": "^9",
    "react": "^18.3.1",
    "rimraf": "^6",
    "tsup": "^8",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  }
