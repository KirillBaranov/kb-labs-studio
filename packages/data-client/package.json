{
  "name": "@kb-labs/data-client",
  "version": "0.1.0",
  "private": true,
  "description": "KB Labs Studio data client â€” API SDK with versioned contracts, mocks, and TanStack Query hooks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "clean": "rimraf dist && find src -name '*.js' -type f ! -path '*/node_modules/*' -delete && find src -name '*.js.map' -type f ! -path '*/node_modules/*' -delete",
    "build": "pnpm clean && tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint . --max-warnings=0",
    "prebuild": "pnpm clean"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.23",
