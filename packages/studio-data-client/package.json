{
  "name": "@kb-labs/studio-data-client",
  "version": "0.1.0",
  "private": true,
  "description": "KB Labs Studio data client â€” API SDK with versioned contracts, mocks, and TanStack Query hooks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist && find src -name '*.js' -type f ! -path '*/node_modules/*' -delete && find src -name '*.js.map' -type f ! -path '*/node_modules/*' -delete",
    "build": "pnpm clean && tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint . --max-warnings=0",
    "prebuild": "pnpm clean"
  },
  "dependencies": {
    "@kb-labs/agent-contracts": "link:../../../kb-labs-agents/packages/agent-contracts",
    "@kb-labs/commit-contracts": "link:../../../kb-labs-commit-plugin/packages/commit-contracts",
    "@kb-labs/core-platform": "link:../../../kb-labs-core/packages/core-platform",
    "@kb-labs/plugin-contracts": "link:../../../kb-labs-plugin/packages/plugin-contracts",
    "@kb-labs/quality-contracts": "link:../../../kb-labs-quality-plugin/packages/quality-contracts",
    "@kb-labs/release-manager-contracts": "link:../../../kb-labs-release-manager/packages/release-manager-contracts",
    "@kb-labs/rest-api-contracts": "link:../../../kb-labs-rest-api/packages/rest-api-contracts",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.13.2",
    "ulid": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@kb-labs/devkit": "github:KirillBaranov/kb-labs-devkit#main",
    "@types/node": "^24.3.3",
    "@types/react": "^18.3.8",
    "eslint": "^9",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  }
}
