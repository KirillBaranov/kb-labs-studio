  },
  "devDependencies": {
    "@ant-design/charts": "^2.6.6",
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.3",
