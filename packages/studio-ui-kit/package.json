{
  "name": "@kb-labs/studio-ui-kit",
  "version": "0.1.0",
  "private": true,
  "description": "KB Labs Studio UI Kit â€” React components using Ant Design and design tokens",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
