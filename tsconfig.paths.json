{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@kb-labs/api-contracts": [
        "../kb-labs-rest-api/packages/api-contracts/src/index.ts"
      ],
      "@kb-labs/api-contracts/*": [
        "../kb-labs-rest-api/packages/api-contracts/src/*"
      ],
      "@kb-labs/data-client": [
        "packages/data-client/src/index.ts"
      ],
      "@kb-labs/data-client/*": [
        "packages/data-client/src/*"
      ],
      "@kb-labs/mind-cli": [
        "../kb-labs-mind/packages/mind-cli/src/index.ts"
      ],
      "@kb-labs/mind-cli/*": [
        "../kb-labs-mind/packages/mind-cli/src/*"
      ],
      "@kb-labs/plugin-adapter-studio": [
        "../kb-labs-plugin/packages/adapters/studio/src/index.ts"
      ],
      "@kb-labs/plugin-adapter-studio/*": [
        "../kb-labs-plugin/packages/adapters/studio/src/*"
      ],
      "@kb-labs/plugin-manifest": [
        "../kb-labs-plugin/packages/manifest/src/index.ts"
      ],
      "@kb-labs/plugin-manifest/*": [
        "../kb-labs-plugin/packages/manifest/src/*"
      ],
      "@kb-labs/ui-core": [
        "packages/ui-core/src/index.ts"
      ],
      "@kb-labs/ui-core/*": [
        "packages/ui-core/src/*"
      ],
      "@kb-labs/ui-react": [
        "packages/ui-react/src/index.ts"
      ],
      "@kb-labs/ui-react/*": [
        "packages/ui-react/src/*"
      ]
    }
  }
}
